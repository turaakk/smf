{
  "name": "SaveMyFeed",
  "version": "0.1.0",
  "description": "Save your youtube feed",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_title": "Save My Feed",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}